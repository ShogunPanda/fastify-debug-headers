{
  "name": "fastify-debug-headers",
  "version": "0.9.0",
  "description": "Fastify plugin to add response debug headers.",
  "homepage": "https://sw.cowtech.it/fastify-debug-headers",
  "repository": "github:ShogunPanda/fastify-debug-headers",
  "keywords": [
    "fastify",
    "fastify-plugin",
    "debug"
  ],
  "bugs": {
    "url": "https://github.com/ShogunPanda/fastify-debug-headers/issues"
  },
  "author": "Shogun <shogun@cowtech.it>",
  "license": "ISC",
  "files": [
    "lib",
    "types",
    "CHANGELOG.md",
    "LICENSE.md",
    "README.md"
  ],
  "main": "lib/index.js",
  "typings": "types/index.d.ts",
  "types": "types/index.d.ts",
  "scripts": {
    "lint": "tslint --project tsconfig.test.json -t stylish src/*.ts test/*.ts",
    "test": "tap --reporter=spec --coverage-report=html --coverage-report=text --no-browser test/*.test.ts",
    "ci": "yarn lint && tap --no-color --reporter=spec --coverage-report=json --coverage-report=text --branches 90 --functions 90 --lines 90 --statements 90 test/*.test.ts",
    "prebuild": "rm -rf lib/* types/* && yarn lint",
    "build": "tsc -p .",
    "prepublishOnly": "yarn ci",
    "postpublish": "git push origin && git push origin -f --tags"
  },
  "dependencies": {
    "fastify-plugin": "^1.6.1"
  },
  "devDependencies": {
    "@cowtech/tslint-config": "^5.13.0",
    "@types/node": "^13.9.0",
    "fastify": "^2.12.1",
    "prettier": "^1.19.1",
    "tap": "^14.10.6",
    "tslint": "^5.20.1",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">=12.15.0"
  }
}
